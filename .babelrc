{
  "env": {
    "development": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "namespace": "ice-cube",
            "minify": false,
            "transpileTemplateLiterals": false
          }
        ]
      ]
    },
    "production": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        [
          "babel-plugin-styled-components",
          {
            "namespace": "ice-cube",
            "displayName": false,
            "pure": true
          }
        ]
      ]
    }
  }
}
